<thlogic>
    <attr sel="#headId" th:replace="~{fragments/head::headFragment}"/>
    <attr sel="#title" th:text="#{game.title}"/>
    <attr sel="#game-over" th:text="#{game.gameOver}"/>
    <attr sel="#result-message" th:text="${__${T(academy.learnprogramming.util.AttributeNames).RESULT_MESSAGE}__}"/>
    <attr sel="#play-link" th:href="@{__${T(academy.learnprogramming.util.GameMappings).RESTART}__}" th:text="#{game.playAgain}"/>
    <attr sel="#home-link" th:href="@{__${T(academy.learnprogramming.util.GameMappings).HOME}__}" th:text="#{game.home}"/>
    <attr sel="#footerId" th:replace="~{fragments/footer::footerFragment}"/>
</thlogic>